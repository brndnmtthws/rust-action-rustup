{
  "author": "Brenden Matthews <github@brenden.brndn.io>",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/tool-cache": "^2.0.1",
    "yn": "^5.0.0"
  },
  "description": "GitHub action for installing rustup",
  "devDependencies": {
    "@tsconfig/node-lts": "^20.1.3",
    "@types/eslint": "^8.56.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.0",
    "@types/prettier": "3.0.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.5",
    "prettier": "3.3.0",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "actions",
    "rust",
    "rustup",
    "cargo"
  ],
  "license": "AGPL-3.0",
  "main": "lib/main.mjs",
  "name": "@brndnmtthws/rust-action-rustup",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brndnmtthws/rust-action-rustup.git"
  },
  "scripts": {
    "act:test": "npm run build && act -j test",
    "build": "tsc && ncc build --source-map --license licenses.txt",
    "format": "prettier --write **/*.{ts,mts,mjs,cjs,json,md,yml}",
    "format-check": "prettier --check **/*.{ts,mts,mjs,cjs,json,md,yml}",
    "lint": "eslint .",
    "prepare": "husky install",
    "test": "jest"
  },
  "module": "true",
  "version": "1.1.0"
}
